<h1>Los productos disponibles son:</h1>

{{#each products}}

<div class="card container-fluid">

    <div class="card-body d-inline-flex p-2">

        <div>

            <p>{{this.title}}</p>
            <p>{{this.description}}</p>
            <p>{{this.price}}</p>

        </div>

    </div>

</div>

{{/each}}

